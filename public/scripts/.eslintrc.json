{   "globals": {
    "_diceValue": true,
    "io": false,
    "THREE": false,
    "Cookies": false,
    "camera": false,
    "dices": true,
    "outlinePass": false,
    "diceCam": false,
    "timer": true,
    "scene": false,
    "mouse": false,
    "renderer": false,
    "composer": false,
    "raycaster": false,
    "ui": false
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-unused-vars": ["error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off"
    }
}
